{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/packages/cspell-types/cspell.schema.json",
  "allowCompoundWords": true,
  "ignorePaths": [
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/**",
    "**/.pnpm-lock.json",
    ".vscode",
    "megalinter",
    "package-lock.json",
    "report"
  ],
  "language": "en,en-GB,en-IN",
  "noConfigSearch": true,
  "useGitignore": true,
  "version": "0.2",
  "words": [
    "aeiou",
    "dpcpy",
    "megalinter",
    "oxsecurity",
    "pady",
    "tearoff",
    "textvariable",
    "venv"
  ]
}
